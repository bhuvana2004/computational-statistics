x=np.array([0,1,3,6,8])
y=np.array([1,3,2,5,4])
x1=0
y1=0
n=5
for i in x:
    x1=i+x1
print(x1)
for i in y:
    y1=i+y1
print(y1)
z=x*y
print(z)
z1=0
for i in z:
    z1=z1+i
print(z1)
x2=x*x
print(x2)
z2=0
for i in x2:
    z2=z2+i
print(z2)
a=((z2*y1)-(x1*z1))/((n*z2)-(x1*x1))
b=((n*z1)-(x1*y1))/((n*z2)-(x1*x1))
print(a)
print(b)
print('y=', a ,'+', b ,'x')
y2=a+b*x
print(y2)
plt.scatter(y2,y)
plt.plot(y2,y)
plt.plot(y2,'o-')
print(y)

plt.show()
    
